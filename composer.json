{
    "name": "sandeep2786498/weight",
    "description": "Weight Checking",
    "keywords": [
        "sandeep2786498",
        "weight"
    ],
    "homepage": "https://github.com/sandeep2786498/weight",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandeep",
            "email": "sandeep2786498@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Spark\\Weight\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spark\\Weight\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}